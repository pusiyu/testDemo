<?xml version="1.0" encoding="UTF-8" ?>
<suite name = "suitename">
    <test name="runAll">
        <classes>
            <class name="com.api.groups.GroupsOnClass1"/>
            <class name="com.api.groups.GroupsOnClass2"/>
            <class name="com.api.groups.GroupsOnClass3"/>
        </classes>
    </test>

    <test name="onlyRunStu">
        <!-- run里include写什么组，就运行那个组 -->
        <groups>
            <run>
                <include name="stu"/>
            </run>
        </groups>
        <classes>
            <class name="com.api.groups.GroupsOnClass1"/>
            <class name="com.api.groups.GroupsOnClass2"/>
            <class name="com.api.groups.GroupsOnClass3"/>
        </classes>
    </test>
</suite>
